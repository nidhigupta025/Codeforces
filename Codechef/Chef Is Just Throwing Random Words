#include <iostream>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    
	    long long a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    
	    long long ans=0,sum=0;
	    for(int i=0;i<n;i++)
	    {
	        ans|=a[i];
	        sum+=a[i];
	        ans|=sum;
	    }
	    cout<<ans<<"\n";
	}
	return 0;
}
